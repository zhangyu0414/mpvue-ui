<template name="uploader$6585d78d">
  <view class="_div data-v-5c0c5ca4 page">
    <view class="_div data-v-5c0c5ca4 page__hd">
      <view class="_div data-v-5c0c5ca4 page__title">Uploader</view>
      <view class="_div data-v-5c0c5ca4 page__desc">上传组件</view>
    </view>
    <view class="_div data-v-5c0c5ca4 page__bd">
      <view class="_div data-v-5c0c5ca4 weui-cells">
        <view class="_div data-v-5c0c5ca4 weui-cell">
          <view class="_div data-v-5c0c5ca4 weui-cell__bd">
            <view class="_div data-v-5c0c5ca4 weui-uploader">
              <view class="_div data-v-5c0c5ca4 weui-uploader__hd">
                <view class="_div data-v-5c0c5ca4 weui-uploader__title">图片上传</view>
                <view class="_div data-v-5c0c5ca4 weui-uploader__info">{{files.length}}/2</view>
              </view>
              <view class="_div data-v-5c0c5ca4 weui-uploader__bd">
                <view class="_div data-v-5c0c5ca4 weui-uploader__files" id="uploaderFiles">
                  <block wx:key="index" key="{{index}}" wx:for="{{files}}" wx:for-index="index" wx:for-item="item">
                    <view class="_div data-v-5c0c5ca4 weui-uploader__file" bindtap="handleProxy" id="{{item}}" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}">
                      <image class="_image data-v-5c0c5ca4 weui-uploader__img" src="{{item}}" mode="aspectFill"></image>
                    </view>
                  </block>
                  <view class="_div data-v-5c0c5ca4 weui-uploader__file">
                    <image class="_image data-v-5c0c5ca4 weui-uploader__img" src="../../../static/images/pic_160.png" mode="aspectFill"></image>
                  </view>
                  <view class="_div data-v-5c0c5ca4 weui-uploader__file">
                    <image class="_image data-v-5c0c5ca4 weui-uploader__img" src="../../../static/images/pic_160.png" mode="aspectFill"></image>
                  </view>
                  <view class="_div data-v-5c0c5ca4 weui-uploader__file">
                    <image class="_image data-v-5c0c5ca4 weui-uploader__img" src="../../../static/images/pic_160.png" mode="aspectFill"></image>
                  </view>
                  <view class="_div data-v-5c0c5ca4 weui-uploader__file weui-uploader__file_status">
                    <image class="_image data-v-5c0c5ca4 weui-uploader__img" src="../../../static/images/pic_160.png" mode="aspectFill"></image>
                    <view class="_div data-v-5c0c5ca4 weui-uploader__file-content">
                      <icon type="warn" size="23" color="#F43530" class="_icon data-v-5c0c5ca4"></icon>
                    </view>
                  </view>
                  <view class="_div data-v-5c0c5ca4 weui-uploader__file weui-uploader__file_status">
                    <image class="_image data-v-5c0c5ca4 weui-uploader__img" src="../../../static/images/pic_160.png" mode="aspectFill"></image>
                    <view class="_div data-v-5c0c5ca4 weui-uploader__file-content">50%</view>
                  </view>
                </view>
                <view class="_div data-v-5c0c5ca4 weui-uploader__input-box">
                  <view class="_div data-v-5c0c5ca4 weui-uploader__input" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}"></view>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</template>